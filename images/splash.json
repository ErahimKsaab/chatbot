  Future getDrivers() async {
    try {
      http.Response response =
          await http.get('http://192.168.1.5:8000/api/gettalldriver');
      if (response.statusCode == 200) {
        setState(() {
          drivers = Searchdrivers = json.decode(response.body);
        });
      } else {
        await Future.delayed(Duration(seconds: 1));
        getDrivers();
      }
    } catch (e) {
      await Future.delayed(Duration(seconds: 1));
      getDrivers();
    }
  }

  Future Getlocation() async {
    bool servise;
    Position per;
    servise = await Geolocator.isLocationServiceEnabled();
    if (servise == false) {
      dialogs(
          click: false,
          F: () async {
            Navigator.pop(context);
            await Future.delayed(Duration(seconds: 2), await Getlocation());
          },
          context: context,
          C: 'TURN ON ',
          t: 'WARNING',
          a: 'OK');
    } else if (await Geolocator.checkPermission() == false) {
      print(await Geolocator.checkPermission());
      Geolocator.requestPermission();
    } else if (await Geolocator.checkPermission() == true) {
      Geolocator.requestPermission();
    }
  }

